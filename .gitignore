# don't commit session and cache data
src/rammerhead/sessions/*
src/rammerhead/cache-js/*
# but commit the empty folders
!src/rammerhead/sessions/.gitkeep
!src/rammerhead/cache-js/.gitkeep
# ignore any custom configurations
/src/rammerhead/config.js
