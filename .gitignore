node_modules/
**/node_modules/**
.env
package=lock.json
pnpm-lock.yaml
yarn.lock
~/

#Rammerhead specific stuff
# don't commit generated builds or env files
src/rammerhead/src/client/*.min.*
src/rammerhead/src/client/hammerhead.js
src/rammerhead/src/client/worker-hammerhead.js
src/rammerhead/src/client/transport-worker.js
